{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" },
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/images", "dest": "/api/images.js" },
    { "src": "/api/images/rating/(.*)", "dest": "/api/images/rating/(.*)" },
    {
      "src": "/api/images/featured",
      "dest": "/api/images/featured"
    },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
